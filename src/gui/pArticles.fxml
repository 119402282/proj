<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" styleClass="borderpane" stylesheets="@resources/css/PatientCSS.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PArticlesController">
   <children>
      <Button fx:id="btnHome" layoutX="396.0" layoutY="500.0" mnemonicParsing="false" onAction="#homeButton" prefHeight="40.0" prefWidth="109.0" text="Home" />
      <Button fx:id="btnInfDisease" layoutX="360.0" layoutY="240.0" mnemonicParsing="false" onAction="#InfDiseaseButton" prefHeight="40.0" prefWidth="180.0" text="Infectious Diseases" />
      <Button fx:id="btnCOVacc" layoutX="360.0" layoutY="320.0" mnemonicParsing="false" onAction="#covidVacButton" prefHeight="40.0" prefWidth="180.0" text="COVID-19 Vaccinations" />
      <Button fx:id="btnAboutCO" layoutX="360.0" layoutY="400.0" mnemonicParsing="false" onAction="#covidButton" prefHeight="40.0" prefWidth="180.0" text="About COVID-19" />
      <AnchorPane prefHeight="200.0" prefWidth="900.0" style="-fx-background-color: EE4E34;">
         <children>
            <ImageView fx:id="imgLogo" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@resources/imgs/logo_inverse.png" />
               </image>
            </ImageView>
            <Text fill="#fcedda" layoutX="335.0" layoutY="118.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Learn More">
               <font>
                  <Font name="System Italic" size="47.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
