<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.Region?>

<BorderPane prefHeight="522.0" prefWidth="900.0" styleClass="borderpane" stylesheets="@resources/css/adminCSS.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AHomeController">
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnVaccReq" layoutX="210.0" layoutY="30.0" mnemonicParsing="false" onAction="#aVaccReqButton" prefHeight="30.0" prefWidth="160.0" text="Vaccine Requests" />
            <Button fx:id="btnVaccCentres" layoutX="530.0" layoutY="30.0" mnemonicParsing="false" onAction="#aVaccCentreButton" prefHeight="30.0" prefWidth="160.0" text="Vaccine Centres" />
            <Button fx:id="btnArticles" layoutX="210.0" layoutY="132.0" mnemonicParsing="false" onAction="#aArticlesButton" prefHeight="30.0" prefWidth="160.0" text="Articles" />
            <Button fx:id="btnVaccInfo" layoutX="530.0" layoutY="132.0" mnemonicParsing="false" onAction="#aVaccInfoButton" prefHeight="30.0" prefWidth="160.0" text="Vaccine Info" />
            <Button fx:id="btnLogOut" layoutX="210.0" layoutY="230.0" mnemonicParsing="false" onAction="#aLogOutButton" prefHeight="30.0" prefWidth="160.0" text="Log Out" />
            <Button layoutX="530.0" layoutY="230.0" mnemonicParsing="false" onAction="#aPatientsButton" prefHeight="30.0" prefWidth="160.0" text="Patients" />
         </children>
      </Pane>
   </center>
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" styleClass="header" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@resources/imgs/logo%20inverse.PNG" />
               </image>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </ImageView>
            <Region prefHeight="200.0" prefWidth="150.0" />
            <Label text="Welcome">
               <HBox.margin>
                  <Insets top="80.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </top>
</BorderPane>
